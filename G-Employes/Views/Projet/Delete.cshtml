@model GestionEmployes.Models.repositories.Projet

@{
    ViewData["Title"] = "Delete";
    Layout = null;
}



<h4>Voulez-vous vraiment supprimer cette projet?</h4>
<div>
    
    <dl class="row">
        <dt class = "col-sm-4">
            Nom de projet :
        </dt>
        <dd class = "col-sm-8">
            @Html.DisplayFor(model => model.Nom_projet)
        </dd>
     
    </dl>
    <form asp-action="Delete" class="d-flex justify-content-center mt-3">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> 
    </form>
</div>
<script src="~/templ/MyJs.js"></script>